{
  "name": "react-github-clone",
  "version": "1.0.1",
  "description": "Boilerplate code to start with React.",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start-dev": "webpack-dev-server --progress --colors --open --config webpack.dev.js",
    "build-dev": "webpack --config webpack.dev.js",
    "build-dev:analyze": "npm run build-dev -- --env.addon=bundleanalyze --env.addon=bundlevisualizer",
    "start-prod": "webpack-dev-server --open --config webpack.prod.js",
    "build-prod": "webpack --config webpack.prod.js -p",
    "build-prod:analyze": "npm run build-prod -- --env.addon=bundleanalyze --env.addon=bundlevisualizer",
    "format": "./node_modules/.bin/prettier --single-quote --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "eslint-check": "./node_modules/.bin/eslint \"src/**/*.js\"",
    "eslint-fix": "./node_modules/.bin/eslint --fix \"src/**/*.js\"",
    "flow": "./node_modules/.bin/flow",
    "flow-check": "./node_modules/.bin/flow check",
    "test": "jest --coverage --passWithNoTests",
    "fomantic-build": "cd semantic && npx gulp build",
    "githook": "npm run eslint-check && npm run test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "engines": {
    "npm": ">=5",
    "node": ">=8.15.1"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "IE 10"
  ],
